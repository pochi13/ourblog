<h1>Blogs#edit</h1>
<p>Find me in app/views/blogs/edit.html.erb</p>
<%= form_with model: @blog, local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> 

  <%= f.label :body %>
  <%= f.text_area :body %> 

  <%= f.label :image %>
  <%= f.attachment_field :image %> 

  <%= f.submit %>

<% end %>
