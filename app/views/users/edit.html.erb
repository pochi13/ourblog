
<%= form_with model: @user, local: true do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %> <br>

  <%= f.label :email %><br>
  <%= f.text_field :email %> <br>

  <%= f.label :profile %><br>
  <%= f.text_area :profile %> <br>

  <%= f.label :profile_image %><br>
  <%= f.attachment_field :profile_image %> <br>

  <%= f.submit %>
<% end %>
