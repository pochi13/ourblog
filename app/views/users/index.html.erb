<div class="row">
  <% @users.each do |user|%>
    <div class="col-4">
      <div class="card" >
        <%= link_to user_path(user) do %>
          <%= attachment_image_tag user, :profile_image, fallback: "no-image.png" %>
          <% end %>
        <div class="card-body">
          <h5 class="card-title">
          <%=link_to user.username, user_path(user) %>
          投稿数：<%= user.blogs.count %>
        </div>
    </div>
  <% end %>
</div>





